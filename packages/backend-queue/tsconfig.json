{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "noImplicitAny": true,
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "strictNullChecks": true,
    "baseUrl": "./",
    "paths": { "*": [ "node_modules/@types/*", "*"] }
  },
  "exclude": [
    "node_modules",
    "dist"
  ]
}
